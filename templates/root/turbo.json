{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "test:watch": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false,
      "dependsOn": ["build"]
    }
  }
}
