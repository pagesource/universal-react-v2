{
  "name": "home",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "bundle": "next build",
    "build": "yarn env-var && yarn bundle",
    "start": "next start",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "test:watch": "yarn test -- --watch",
    "test": "jest -c ./config/jest/jest.config.js",
    "bundlesize": "bundlesize --config ./config/bundlesize.json",
    "analyze": "cross-env ANALYZE=true && yarn build",
    "perf": "docker-compose -f ./config/performance/sitespeed-compose.yml run sitespeed.io  --config sitespeed.config.json --budget.configPath sitespeed.budget.config.json --graphite.host=graphite urls.txt",
    "env-var": "cross-env BASE_PATH=/docs"
  },
  "browserslist": [
    "> 0.25%",
    "not ie <= 11",
    "not edge < 15",
    "not op_mini all",
    "not dead"
  ],
  "dependencies": {
    "@mdx-js/react": "^1.6.22",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "next": "*",
    "@xt-pagesource/micro-app-loader": "0.1.0",
    "markdown-to-jsx": "^7.1.3",
    "react": "*",
    "react-dom": "*",
    "themes": "*",
    "services": "*",
    "ui": "*",
    "smart-context": "^2.1.2",
    "@stitches/react": "^1.2.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "11.1.2",
    "@babel/plugin-transform-runtime": "7.15.0",
    "next-transpile-modules": "9.0.0",
    "config": "*",
    "eslint": "7.32.0",
    "tsconfig": "*",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.39",
    "typescript": "^4.5.3",
    "@types/react-dom": "^17.0.11",
    "@testing-library/react": "12.1.1",
    "@testing-library/react-hooks": "7.0.2",
    "bundlesize": "0.18.1",
    "cross-env": "7.0.3",
    "lintconfig": "*"
  }
}
