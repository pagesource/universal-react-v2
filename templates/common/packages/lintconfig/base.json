{
  "extends": [
    "airbnb",
    "airbnb/hooks",
    "airbnb-typescript",
    "plugin:@next/next/recommended",
    "prettier"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "react/function-component-definition": "off",
    "no-restricted-exports": "off",
    "react/jsx-props-no-spreading": "off"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  }
}
